}

#header_menu_cont .icon {

  display: none;

  background: transparent url('../images/menu_icon.png') center center no-repeat;

  background-size: 2em 2em;

  height: 3.2em;

  position: absolute;

  right: 0.5em;

  top: 0;

  width: 4.4em;

}

#header_menu_cont .logo {

  background: transparent url('../images/logo.png') center center no-repeat;

  background-size: 100% 100%;

  display: block;

  float: left;

  height: 3.125em;

  margin: 0 1em 0.5em 0.5em;

  width: 16.5625em;

}

#header_w {

  background: #333 url('../images/bg_header.jpg') center center no-repeat;

  background-size: cover;

  padding: 1.5625em 0 0 0;

}

#header {

  color: #FFF;

  padding: 3.625em 0 6.792em 0;

  position: relative;

}

#header_menu {

  font-size: 0.8125em;

  text-align: right;

}

#header_menu li {

  background: rgba(40, 40, 40, 0.9);

  display: block;

  line-height: 2;

  position: relative;

  text-align: left;

  white-space: nowrap;

}

#header_menu a {

  display: block;

  padding: 0 0.6em 0 1.2em;

}

#header_menu ul {

  display: none;

  left: 0;

  min-width: 100%;

  position: absolute;

  top: 100%;

  z-index: 2;

}

#header_menu > li {

  background: none;

  display: inline-block;

  margin: 0.5em 0.3em;

  padding: 0;

}

#header_menu > li > a {

  padding-left: 0.2em;

  padding-right: 0.2em;

}

#header_menu li.sub > a {

  padding-left: 1.2em;

  padding-right: 0.2em;

}

#header_menu li.sub a:hover {

  text-decoration: underline;

}

#header_menu > li.sub > a:hover {

  text-decoration: none;

}

#header_menu li.sub > a:before {

  content: ' ';

  border-color: #FFFFFF transparent transparent transparent;

  border-style: solid;

  border-width: 0.375em 0.375em 0 0.375em;

  height: 0;

  left: 0.125em;

  position: absolute;

  top: 0.9em;

  width: 0;

}

#header_menu li.sub.sel > a:before {

  border-color: transparent transparent transparent #FFFFFF;

  border-width: 0.375em 0 0.375em 0.375em;

  left: 0.4em;

  top: 0.7em;

}

#header_menu li.sub.sel > ul {

  display: block;

}

#header_menu .btn {

  background: #E00000;

  -webkit-border-radius: 0.3em;

  -moz-border-radius: 0.3em;

  border-radius: 0.3em;

  padding-left: 0.4em;

  padding-right: 0.4em;

}

#header_menu .search {

  background: transparent url('../images/icon_search.png') center center no-repeat;

  background-size: 76.923% auto;

  height: 2em;

  padding: 0;

  width: 2em;

}

#header_menu .search_cont {

  float: right;

  margin-right: 1em;

}

#header_menu .search_form {

  display: none;

  padding: 0.6em 0 0 0;

  position: absolute;

  right: 0;

  top: 100%;

}

#header_menu .search_form input {

  border: 1px solid #DADADA;

  -webkit-border-radius: 0.3em;

  -moz-border-radius: 0.3em;

  border-radius: 0.3em;

  padding: 0.2em;

  text-align: right;

}



