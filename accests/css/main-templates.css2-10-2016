a.arrow {

  color: #e88787;

  display: inline-block;

  line-height: 1.2;

  padding: 0 1.6em 0 0;

  position: relative;

}

a.arrow:hover{color:#333;}



a.arrow:before {

  background: transparent url('../images/icon_link_arrow.png') center center no-repeat;

  background-size: 100% 100%;

  content: ' ';

  height: 1.3125em;

  position: absolute;

  right: 0;

  top: -0.0625em;

  width: 1.3125em;

}

a.arrow1:before { background-image: url('../images/icon_link_arrow1.png'); }

a.arrow2 {

  line-height: 2.4;

  padding-left: 0.6em;

  padding-right: 2.2em;

}

a.arrow2:before {

  top: 0.5375em;

  right: 0.6em;

}



/* header */

#header_menu_cont {

  color: #FFF;

